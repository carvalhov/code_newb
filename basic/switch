#include <iostream>
using namespace std;
int main()
{
int sorvete; 
cin >> sorvete;
cout << "Sabor ";
switch(sorvete){
case 1: 
cout << "Napolitano";
break;
case 2: 
cout << "chocolate";
break;
case 3:
cout << "Baunilha";
break;
default: 
cout << "indisponÃ­vel";
break;
}
}
