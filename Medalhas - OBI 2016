#include <iostream>
using namespace std;

void ordem (int a, int b, int c) {
  if (a < b and a < c){
  if (b < c) {
    cout << "1\n2\n3";
    }
  if (c < b) {
    cout << "1\n3\n2";
    }
  }
  if (b < a and b < c) {
  if (a < c){
    cout << "2\n1\n3";
  }
  if (c < a) {
    cout << "2\n3\n1";
    }
  }
  if (c < a and c < b){
    if (a < b) {
    cout << "3\n1\n2";
    }
    if (b < a) {
    cout << "3\n2\n1";
  } 
}
}

int main(){
int t1, t2, t3; // t1 = 1, t2 = 2 , t3 = 3 
cin >> t1 >> t2 >> t3;

ordem (t1, t2, t3);

return 0;
}
